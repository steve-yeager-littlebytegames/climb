@model Climb.ViewModels.Account.AccessDeniedViewModel

@{
    ViewBag.Title = "Access Denied";
}

<h1 class="mb-5">Access Denied</h1>

<div>You've been parried! You do not have access to @Model.ReturnUrl.</div>
<div>Maybe you are on the wrong account?</div>
<form class="mt-5" asp-controller="Account" asp-action="LogOut">
    <button class="btn btn-cstm-primary">Log Out</button>
</form>